<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_splash"
    app:startDestination="@+id/FRSplash">

    <fragment
        android:id="@+id/FRSplash"
        android:name="com.example.spacex.ui.splash.FRSplash"
        android:label="@string/splash_screen"
        tools:layout="@layout/fr_splash">
        <action
            android:id="@+id/toFRHome"
            app:destination="@id/FRHome" />
    </fragment>

    <fragment
        android:id="@+id/FRHome"
        android:name="com.example.spacex.ui.home.FRHome"
        android:label="Home" />

</navigation>